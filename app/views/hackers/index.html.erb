<%= form_for @ken, url: hackers_path, html:{class: "user_form"} do |f| %>
  <label>Name:</label>
  <%= f.text_field :name %>
  <label>Email:</label>
  <%= f.text_field :email %>
  <%= f.submit "Create Account" %>
<% end %>
